{
  "expo": {
    "cli": {
      "appVersionSource": "remote"
    },
    "name": "MkulimaNetMobileExpo",
    "slug": "mkulima-net",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mkulimanetmobileexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/a94a127e-aaf8-4f52-976c-3e7b4bf64973"
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mkulimanet.mobile",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "INTERNET"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "MkulimaNet - Agricultural Network",
      "shortName": "MkulimaNet",
      "description": "Connect with farmers, find jobs, buy/sell agricultural products",
      "themeColor": "#1B5E20",
      "backgroundColor": "#FFFFFF",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "preferRelatedApplications": false,
      "icon": {
        "src": "./assets/images/icon.png",
        "sizes": [48, 72, 96, 128, 144, 152, 192, 384, 512]
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1B5E20"
      }
    },
    "extra": {
      "eas": {
        "projectId": "a94a127e-aaf8-4f52-976c-3e7b4bf64973"
      },
      "projectId": "a94a127e-aaf8-4f52-976c-3e7b4bf64973",
      "router": {}
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "manjoe12"
  }
}
